	pages = {2684--2690},
	file = {Full Text PDF:/home/julian/.zotero/zotero/2db7rio7.default/zotero/storage/HRRNM9TJ/Hutchins et al. - 2008 - Position-dependent motif characterization using no.pdf:application/pdf;Snapshot:/home/julian/.zotero/zotero/2db7rio7.default/zotero/storage/VDG6BU5J/2684.html:text/html}
